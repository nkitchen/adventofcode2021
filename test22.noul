test := \ -> (
    c := [[1, 5, 9], [3, 7, 11]];
    d := [[2, 6, 10], [4, 8, 12]];
    x := c cubisect d;
    assert (x == [[2, 6, 10], [3, 7, 11]]);
    print("ok cubisect");

    cs := x sunder c;
    exp := [
        [[1, 5, 9],  [1, 5, 11]],
        [[1, 6, 9],  [1, 7, 11]],
        [[2, 5, 9],  [3, 5, 11]],
        [[2, 6, 9],  [3, 7, 11]],
    ];
    assert (set(cs) == set(exp));
    print("ok sunder");

    f := "ex22a.txt";
    n := init_cubes(f);
    print(F"ex22a {n}");
    assert (n == 39);
    print("ok ex22a.txt");

    f = "ex22b.txt";
    n = init_cubes(f);
    assert (n == 590784);
    print("ok ex22b.txt");

    print("PASS");
);
